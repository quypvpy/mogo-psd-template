section.story
    .container
        +blockBox("Out stories","Latest blog")
        ul.story-list
            +storyItem("https://images.unsplash.com/photo-1644982648791-a010e61aa845?ixlib=rb-1.2.1&ixid=MnwxMjA3fDF8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=870&q=80",2,"Nov")
